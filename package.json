{
  "name": "page",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "compile:sass": "sass src/scss/main.scss src/style.css",
    "watch:sass": "sass src/scss/main.scss src/style.css --watch",
    "start": "npm-run-all --parallel dev watch:sass",

    "devserver": "live-server",
    "start:css": "npm-run-all --parallel devserver watch:sass",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "sass <css/style.prefix.css> [css/style.css] compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "hexo-server": "^3.0.0",
    "sass": "^1.62.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "hexo-githubcalendar": "^1.2.3",
    "hexo-leetcode-calendar": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "vite": "^4.3.0",
    "vuex": "^4.1.0"
  }
}
